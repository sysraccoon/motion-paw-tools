{
  "name": "@sysraccoon/motion-paw-tools",
  "version": "0.0.3",
  "description": "",
  "license": "MIT",
  "scripts": {
    "build:dev": "rollup -c rollup.config.mjs",
    "watch": "rollup -c rollup.config.mjs -w",
    "build": "rollup -c rollup.config.mjs",
    "prepare": "npm run build",
    "prebuild": "rimraf ./lib",
    "lint-staged": "lint-staged",
    "lint": "npm run eslint && npm run prettier",
    "format": "npm run eslint:fix && npm run prettier:fix",
    "eslint": "eslint \"**/*.ts?(x)\"",
    "eslint:fix": "eslint --fix \"**/*.ts?(x)\"",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "serve": "vite"
  },
  "unpkg": "lib/bundle.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "motion-canvas"
  ],
  "author": "sysraccoon <sysraccoon@gmail.com>",
  "homepage": "",
  "peerDependencies": {
    "@motion-canvas/2d": "^3.17.1",
    "@motion-canvas/core": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@motion-canvas/ui": "^3.17.0",
    "@motion-canvas/vite-plugin": "^3.17.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.0",
    "@types/node": "^22.9.0",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.18.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-tsdoc": "^0.3.0",
    "globals": "^15.10.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-node-externals": "^7.1.3",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.1",
    "vite": "^4.2.1",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{js,jsx,ts,tsx,md,scss,json,mjs}": "prettier --write"
  },
  "dependencies": {
    "@fig/lezer-bash": "^1.2.5",
    "@hhenrichsen/canvas-commons": "^0.10.2",
    "@lezer/javascript": "^1.4.19",
    "qrcode": "^1.5.4"
  }
}
